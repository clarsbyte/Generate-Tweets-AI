# Add additional patterns to your .gitignore
Add-Content .gitignore @"

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
env/
venv/
ENV/
env.bak/
venv.bak/

# Node binary files
*.node

node_modules/

# OS files
Thumbs.db
.DS_Store

# IDE files
.vscode/
.idea/
*.swp
*.swo

# Logs
*.log
logs/

# Database
*.db
*.sqlite3

# Temporary files
*.tmp
*.temp
"@